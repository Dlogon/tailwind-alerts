{
    "name": "dlogon/tailwind-alerts",
    "description": "create tailwind alerts from static",
    "keywords": [
        "toast",
        "tailwind-alerts",
        "laravel"
    ],
    "homepage": "https://github.com/Dlogon/tailwind-alerts",
    "license": "MIT",
    "authors": [
        {
            "name": "Diego Lopez",
            "email": "dlogon@outlook.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Dlogon\\TailwindAlerts\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dlogon\\TailwindAlerts\\Tests\\": "tests"
        }
    },
    "scripts": {

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dlogon\\TailwindAlerts\\TailwindAlertsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
